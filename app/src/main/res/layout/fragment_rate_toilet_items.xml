<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:cardBackgroundColor="#FE6600"
    app:cardCornerRadius="40dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:orientation="horizontal"
            android:layout_weight="1"
            android:layout_marginBottom="3dp"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp">

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_gravity="center_vertical">

                <android.support.v7.widget.CardView
                    android:id="@+id/water_card"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="40dp"
                    android:layout_centerHorizontal="true">

                    <ImageView
                        android:id="@+id/water_image"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="30dp"
                        android:layout_gravity="center"
                        android:src="@drawable/water"/>

                </android.support.v7.widget.CardView>

                <android.support.v7.widget.AppCompatButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Water"
                    android:textSize="18sp"
                    app:elevation="14dp"
                    android:layout_centerHorizontal="true"
                    android:layout_below="@+id/water_card"
                    android:background="@drawable/rounded_corner_rate_toilet"
                    android:layout_gravity="center_horizontal"/>

            </RelativeLayout>


            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_gravity="center_vertical">

                <android.support.v7.widget.CardView
                    android:id="@+id/dustbin_card"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    app:cardCornerRadius="40dp">

                    <ImageView
                        android:id="@+id/dustbin_image"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="32dp"
                        android:layout_marginBottom="32dp"
                        android:layout_marginRight="27dp"
                        android:layout_marginLeft="27dp"
                        android:layout_gravity="center"
                        android:src="@drawable/dustbin"/>

                </android.support.v7.widget.CardView>

                <android.support.v7.widget.AppCompatButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:text="Dustbin"
                    app:elevation="15dp"
                    android:textSize="18sp"
                    android:layout_below="@+id/dustbin_card"
                    android:background="@drawable/rounded_corner_rate_toilet" />

            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:orientation="horizontal"
            android:layout_weight="1"
            android:layout_marginTop="3dp"
            android:layout_marginBottom="3dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp">

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_gravity="center_vertical">

                <android.support.v7.widget.CardView
                    android:id="@+id/soap_card"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="40dp"
                    android:layout_centerHorizontal="true">

                    <ImageView
                        android:id="@+id/soap_image"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="31dp"
                        android:src="@drawable/soap"/>

                </android.support.v7.widget.CardView>

                <android.support.v7.widget.AppCompatButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Soap"
                    android:textSize="18sp"
                    app:elevation="15dp"
                    android:layout_centerHorizontal="true"
                    android:layout_below="@+id/soap_card"
                    android:background="@drawable/rounded_corner_rate_toilet"
                    android:layout_gravity="center_horizontal"/>

            </RelativeLayout>


            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_gravity="center_vertical">

                <android.support.v7.widget.CardView
                    android:id="@+id/mirror_card"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    app:cardCornerRadius="40dp">

                    <ImageView
                        android:id="@+id/mirror_image"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="29dp"
                        android:src="@drawable/mirror"/>

                </android.support.v7.widget.CardView>

                <android.support.v7.widget.AppCompatButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:text="Mirror"
                    app:elevation="15dp"
                    android:textSize="18sp"
                    android:layout_below="@+id/mirror_card"
                    android:background="@drawable/rounded_corner_rate_toilet" />

            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:orientation="horizontal"
            android:layout_weight="1"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="8dp"
            android:layout_marginBottom="3dp"
            android:layout_marginRight="8dp">

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginLeft="12dp"
                android:layout_gravity="center_vertical">

                <android.support.v7.widget.CardView
                    android:id="@+id/towel_card"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    app:cardCornerRadius="40dp">

                    <ImageView
                        android:id="@+id/towel_image"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:layout_marginTop="33dp"
                        android:layout_marginBottom="33dp"
                        android:src="@drawable/towels"/>

                </android.support.v7.widget.CardView>

                <android.support.v7.widget.AppCompatButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Towels"
                    android:textSize="18sp"
                    app:elevation="15dp"
                    android:layout_below="@+id/towel_card"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/rounded_corner_rate_toilet"
                    android:layout_gravity="center_horizontal"/>

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_gravity="center_vertical">

                <android.support.v7.widget.CardView
                    android:id="@+id/water_basin_card"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    app:cardCornerRadius="40dp">

                    <ImageView
                        android:id="@+id/petrol_pump_card"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="23dp"
                        android:layout_marginRight="23dp"
                        android:layout_marginTop="32dp"
                        android:layout_marginBottom="32dp"
                        android:src="@drawable/water_basin"/>

                </android.support.v7.widget.CardView>

                <android.support.v7.widget.AppCompatButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:text="Water Basin"
                    android:textSize="18sp"
                    app:elevation="15dp"
                    android:layout_below="@+id/water_basin_card"
                    android:background="@drawable/rounded_corner_rate_toilet" />

            </RelativeLayout>
        </LinearLayout>

    </LinearLayout>




</android.support.v7.widget.CardView>